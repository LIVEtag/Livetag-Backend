{
  "name": "gbksoft/yii2-base",
  "description": "Yii 2 Base",
  "type": "project",
  "license": "MTI",
  "minimum-stability": "stable",
  "require": {
    "php": "~7.1.0",
    "yiisoft/yii2": "2.0.11.2",
    "yiisoft/yii2-bootstrap": "2.0.6",
    "yiisoft/yii2-swiftmailer": "2.0.7",
    "fxp/composer-asset-plugin": "1.3.1",
    "gbksoft/shell-command-builder": "1.0.1",
    "yiisoft/yii2-authclient": "2.1.2",
    "roave/security-advisories": "dev-master"
  },
  "require-dev": {
    "yiisoft/yii2-debug": "2.0.9",
    "yiisoft/yii2-gii": "2.0.5",
    "yiisoft/yii2-faker": "2.0.3",
    "andres-montanez/magallanes": "3.0",
    "phpmd/phpmd": "2.6",
    "sebastian/phpcpd": "2.0.4",
    "squizlabs/php_codesniffer": "2.9.*",
    "phpunit/phpunit": "4.8.9",
    "consolidation/robo": "1.0.6"
  },
  "autoload-dev": {
    "psr-4": {
      "robo\\": "dev/robo"
    }
  },
  "config": {
    "process-timeout": 1800
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  }
}
