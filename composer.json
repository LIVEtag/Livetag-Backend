{
    "name": "gbksoft/yii2-base",
    "description": "Yii 2 Base",
    "type": "project",
    "license": "MTI",
    "minimum-stability": "stable",
    "require": {
        "php": "~7.1.0",
        "yiisoft/yii2": "2.0.12",
        "yiisoft/yii2-bootstrap": "2.0.6",
        "yiisoft/yii2-swiftmailer": "2.0.7",
        "yiisoft/yii2-authclient": "2.1.3",
        "gbksoft/shell-command-builder": "1.0.1",
        "kartik-v/yii2-widget-select2": "2.1.0",
        "kosinix/grafika": "2.0.8",
        "frostealth/yii2-aws-s3": "2.1.0",
        "kartik-v/yii2-widgets": "3.4.0",
        "gbksoft/phpcent": "~1.0.5"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "2.0.9",
        "yiisoft/yii2-gii": "2.0.5",
        "yiisoft/yii2-faker": "2.0.3",

        "codeception/base": "2.3.4",
        "codeception/verify": "0.4.0",

        "phpmd/phpmd": "2.6",
        "sebastian/phpcpd": "2.0.4",
        "squizlabs/php_codesniffer": "2.9.*",
        "phpunit/phpunit": "4.*",
        "consolidation/robo": "1.0.6"
    },

    "autoload-dev": {
        "psr-4": {
            "robo\\": "dev/robo"
        }
    },

    "config": {
        "process-timeout": 1800,
        "fxp-asset":{
            "installer-paths": {
                "npm-asset-library": "vendor/npm",
                "bower-asset-library": "vendor/bower"
            }
        }
    }
}
